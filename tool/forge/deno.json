{
  "name": "@roka/forge",
  "version": "0.1.5",
  "exports": {
    ".": "./forge.ts",
    "./bump": "./bump.ts",
    "./changelog": "./changelog.ts",
    "./compile": "./compile.ts",
    "./package": "./package.ts",
    "./release": "./release.ts",
    "./testing": "./testing.ts",
    "./version": "./version.ts"
  },
  "forge": {
    "main": "forge.ts"
  },
  "permissions": {
    "default": {
      "env": ["GIT_NAME", "GIT_EMAIL", "GITHUB_TOKEN", "HOME"],
      "read": true,
      "write": true,
      "net": ["api.github.com"],
      "run": ["deno", "git", "tar", "zip"]
    }
  },
  "test": {
    "permissions": {
      "read": true,
      "write": true,
      "run": true,
      "env": ["GITHUB_TOKEN", "HOME"],
      "net": ["api.github.com"]
    }
  }
}
