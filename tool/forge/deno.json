{
  "name": "@roka/forge",
  "version": "0.1.0-pre.26+07aea36",
  "exports": {
    ".": "./forge.ts",
    "./bump": "./bump.ts",
    "./changelog": "./changelog.ts",
    "./compile": "./compile.ts",
    "./package": "./package.ts",
    "./release": "./release.ts",
    "./testing": "./testing.ts",
    "./version": "./version.ts"
  },
  "compile": {
    "main": "forge.ts",
    "permissions": {
      "env": [
        "GIT_NAME",
        "GIT_EMAIL",
        "GITHUB_TOKEN",
        "HOME"
      ],
      "read": true,
      "write": true,
      "net": "api.github.com",
      "run": [
        "deno",
        "git",
        "tar",
        "zip"
      ],
      "prompt": true
    }
  }
}
